00:0000DBAA 7253            	   128: 	MOVEQ		#$53,d1			; [][][][S] key
00:0000DBAC 7013            	   129: 	MOVEQ		#19,d0			; check for keypress
00:0000DBAE 4E4F            	   130: 	TRAP		#15
                            	   131: 
00:0000DBB0 102B0014        	   132: 	MOVE.b	s_key(a3),d0		; get the last key(s) state
00:0000DBB4 B300            	   133: 	EOR.b		d1,d0				; compare the result with the last key(s) state,
                            	   134: 							; each byte is now $FF if a key has changed or
                            	   135: 							; $00 if a key has not changed
00:0000DBB6 C001            	   136: 	AND.b		d1,d0				; make each byte $FF if key just pressed or
                            	   137: 							; $00 if key not just pressed
00:0000DBB8 B12B0015        	   138: 	EOR.b		d0,play_sound(a3)		; if key just pressed toggle the sound flag
00:0000DBBC 17410014        	   139: 	MOVE.b	d1,s_key(a3)		; save the last [S] key status
00:0000DBC0 4E75            	   140: 	RTS

                            	  1677: ;==============================================================================
                            	  1678: ; PLIC - platform level interrupt controller
                            	  1679: ;
                            	  1680: ; Register layout:
                            	  1681: ;   bits 0 to 7  = cause code to issue (vector number)
                            	  1682: ;   bits 8 to 11 = irq level to issue
                            	  1683: ;   bit 16 = irq enable
                            	  1684: ;   bit 17 = edge sensitivity
                            	  1685: ;   bit 18 = 0=vpa, 1=inta
                            	  1686: ;		bit 24 to 29 target core
                            	  1687: ;
                            	  1688: ; Note byte order must be reversed for PLIC.
                            	  1689: ;==============================================================================
                            	  1690: 

                            	   556: InitMMU:
                            	   557: 	lea MMU+8,a0				; first 128kB is local RAM
                            	   558: 	move.l #$32000,d2		; map all pages to DRAM
                            	   559: 	move.l #510,d0			; then override for IO later
                            	   560: .0002
                            	   561: 	move.l d2,d1
                            	   562: 	bsr rbo
                            	   563: 	move.l d1,(a0)+
                            	   564: 	addi.w #1,d2				; increment DRAM page number
                            	   565: 	dbra d0,.0002
                            	   566: 	lea MMU,a0					; now program IO access

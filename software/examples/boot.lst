00:00006D8C 2E80            	  1413: 	move.l d0,(sp)				; push data type
00:00006D8E F22F68000004    	  1414: 	fmove.x fp0,4(sp)			; and value
00:00006D94 4ED1            	  1415: 	jmp (a1)
